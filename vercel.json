{
  "headers": [
    {
      "source": "/(.*)\\.(webp|png|jpg|jpeg|svg|gif|js|css|woff2|woff|ttf|eot)$",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    },
    {
      "source": "/(.*)\\.html$",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=600, must-revalidate" }
      ]
    }
  ]
} 